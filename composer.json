{
    "name": "webboy/open-ai-api-client",
    "description": "PHP client for the OpenAI API",
    "type": "library",
    "license": "MIT",
    "version": "1.0.2",
    "autoload": {
        "psr-4": {
            "Webboy\\OpenAiApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nemanja Milenkovic",
            "email": "extramedia.nemanja@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5@dev",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.5@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1@dev",
        "squizlabs/php_codesniffer": "*",
        "infection/infection": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
